layout-macro-def (knot-details){
	params{
		param (knot_here){
			type (Knot_struct)
			min (Required)
			max (One)
		}
	}
	content{
		section{
			content{
				for-each (knot_here.mult_pic){
					as(pic){
						image{
							aspect-ratio(4:3)
							object-fit (Contain)
							lightbox-enabled (false)
							url("#{value(pic.url)}") //Not showing picture.
						}
						paragraph{
							value("#{value(pic.text)}")
						}
					}
				}
			}
		}
	}
}